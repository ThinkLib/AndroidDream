<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <corners android:radius="1dp" />
            <gradient
                android:startColor="#ff5a5d5a"
                android:centerColor="#ff5a5d5a"
                android:centerY="0.75"
                android:endColor="#ff5a5d5a"
                android:angle="270"
                />
        </shape>
    </item>

    <item>
        <clip>
            <shape>
                <corners android:radius="1dp" />
                <gradient
                    android:startColor="#ff5a5d5a"
                    android:centerColor="#ff5a5d5a"
                    android:centerY="0.75"
                    android:endColor="#ff5a5d5a"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>

    <item>
        <clip>
            <shape>
                <corners android:radius="1dp" />
                <gradient
                    android:startColor="#44C494"
                    android:centerColor="#44C494"
                    android:centerY="0.75"
                    android:endColor="#44C494"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>
</layer-list>